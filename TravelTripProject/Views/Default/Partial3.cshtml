@using TravelTripProject.Models.Entity
@model List<Blog> 

<div class="padding">
    <h4>This Week's Top 10</h4>
    <ul class="list list-pad">
        @{int i = 0; }
        @foreach (var item in Model)
        {
            <li class="list-bg"><span>@{i++;} @i.</span><a href="/Blog/BlogDetails/@item.ID">@item.Title</a></li>
        }
    </ul>
    <a href="#" class="link1">See all Blogs</a>
</div>
