@model List<TravelTripProject.Models.Entity.Contact>
@{
    ViewBag.Title = "Contact";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Message List</h2>
<br />
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>NAME</th>
        <th>EMAIL</th>
        <th>TOPIC</th>
        <th>MESSAGE</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ID</td>
            <td>@item.Name</td>
            <td>@item.Email</td>
            <td>@item.Topic</td>
            <td>@item.Message</td>
        </tr>
    }

</table>